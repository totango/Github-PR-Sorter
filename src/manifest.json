{
  "manifest_version": 2,

  "icons": {
	"48": "img/icon48.png"
  },

  "name": "Github PR Sorter",
  "description": "Github Pull Request Sorter",
  "version": "1.4",

  "content_scripts": [
	{
	  "js": ["jquery.min.js", "moment.min.js", "jqueryWhenExtended.js", "prSorter.js", "main.js"],
	  "matches": ["https://github.com/*"],
	  "run_at": "document_end"
	}
  ],

  "permissions": [
  ]
}